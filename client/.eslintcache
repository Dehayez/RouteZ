ates\n  const [ liked, setLiked ] = useState(material && material._likeIds.includes(user));\n\n  // Like the material\n  const likeMaterial = async () => {\n    setLiked(!liked);\n\n    await addLikeToMaterial(token, user, material._id);\n  };\n\n  // Dislike the material\n  const dislikeMaterial = async () => {\n    setLiked(!liked);\n\n    await addDislikeToMaterial(token, user, material._id);\n  };\n\n  // Download material\n  const readMaterial = () => {\n    getDoc(material.file);\n  };\n\n  return (\n    <>\n    {/** Liked or naaaaah */}\n    {\n      liked ? (\n        <button onClick={dislikeMaterial}>\n          Unlike\n        </button>\n      ) : (\n        <button onClick={likeMaterial}>\n          Like\n        </button>\n      )\n    }\n    <NavLink to={`${Routes.MATERIAL.replace(':name', material.id)}`}>\n      Als je hierop klikt, dan zie je meer...\n    </NavLink>\n    {/** Download material */}\n    <span onClick={readMaterial}>Download</span>\n    </>    \n  );\n};\n\nexport default CardMaterial;\n","/Users/jensderyckere/Sites/RouteZ/client/src/components/links/index.js",[],"/Users/jensderyckere/Sites/RouteZ/client/src/partials/paths/index.js",[],"/Users/jensderyckere/Sites/RouteZ/client/src/partials/paths/Exercises.js",[],"/Users/jensderyckere/Sites/RouteZ/client/src/partials/paths/TipsAndTricks.js",[],"/Users/jensderyckere/Sites/RouteZ/client/src/partials/paths/Video.js",[],"/Users/jensderyckere/Sites/RouteZ/client/src/partials/paths/Theory.js",[],"/Users/jensderyckere/Sites/RouteZ/client/src/components/links/BackLinks.js",[],"/Users/jensderyckere/Sites/RouteZ/client/src/services/toolbox.service.js",["227"],"import React, { createContext, useContext } from 'react';\nimport { useHistory } from 'react-router-dom';\n\nconst ToolboxContext = createContext();\nconst useToolbox = () => useContext(ToolboxContext);\n\nconst ToolboxProvider = ({children}) => {\n  return (\n    <ToolboxContext.Provider value={{\n    }}>\n      {children}\n    </ToolboxCon